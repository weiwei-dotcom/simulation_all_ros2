<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:include filename="$(find mz_description)/xacro/transmission.xacro"/>

    <xacro:macro name="leg" params="name mirror mirror_dae front_hind front_hind_dae">

        <joint name="${name}_hip_joint" type="revolute">
            <!-- FR-->
            <xacro:if value="${(mirror_dae == False) and (front_hind_dae == True)}">
                <origin xyz="0.22625 -0.07 0.075" rpy="0 0 0" />
                <axis xyz="1 0 0" />
            </xacro:if>
            <!-- FL-->
            <xacro:if value="${(mirror_dae == True) and (front_hind_dae == True)}">
                <origin xyz="0.22625 0.07 0.075" rpy="0 0 0" />
                <axis xyz="1 0 0" />
            </xacro:if>
            <!-- RR-->
            <xacro:if value="${(mirror_dae == False) and (front_hind_dae == False)}">
                <origin xyz="-0.22625 -0.07 0.075" rpy="0 0 0" />
                <axis xyz="-1 0 0" />
            </xacro:if>
            <!-- RL-->
            <xacro:if value="${(mirror_dae == True) and (front_hind_dae == False)}">
                <origin xyz="-0.22625 0.07 0.075" rpy="0 0 0" />
                <axis xyz="-1 0 0" />
            </xacro:if>

            <parent link="trunk" />
            <child link="${name}_hip" />

            <dynamics damping="0.01" friction="0.2"/>
            <limit effort="18" lower="-3.14" upper="3.14" velocity="40"/>

        </joint>

        <link name="${name}_hip">
            <visual>
                <!-- FR-->
                <xacro:if value="${(mirror_dae == False) and (front_hind_dae == True)}">
                    <origin xyz="0 0 0" rpy="0 0 0" />
                </xacro:if>
                <!-- FL-->
                <xacro:if value="${(mirror_dae == True) and (front_hind_dae == True)}">
                    <origin xyz="0 0 0" rpy="3.1415 0 0" />
                </xacro:if>
                <!-- RR-->
                <xacro:if value="${(mirror_dae == False) and (front_hind_dae == False)}">
                    <origin xyz="0 0 0" rpy="3.1415 0 3.1415" />
                </xacro:if>
                <!-- RL-->
                <xacro:if value="${(mirror_dae == True) and (front_hind_dae == False)}">
                    <origin xyz="0 0 0" rpy="0 0 3.1415" />
                </xacro:if>
                <geometry>
                    <mesh filename="package://mz_description/meshes/hip.dae" />
                </geometry>
                <material name="">
                    <color rgba="0.75294 0.75294 0.75294 1" />
                </material>
            </visual>
            <collision>
                <origin rpy="1.57079632679 0 0" xyz="0 0 0"/>
                <geometry>
                    <cylinder length="0.04" radius="0.06"/>
                </geometry>
            </collision>
            <inertial>
                <origin xyz="-0.0037743 0.0038894 7.8535E-05" rpy="0 0 0" />
                <mass value="0.68" />
                <inertia ixx="0.000334008405" ixy="-0.000010826066" ixz="0.000001290732" iyy="0.000619101213" iyz="0.000001643194" izz="0.00040057614" />
            </inertial>
        </link>
        <joint name="${name}_thigh_joint" type="revolute">
            <!-- FR-->
            <xacro:if value="${(mirror_dae == False) and (front_hind_dae == True)}">
                <origin xyz="0.08775 -0.02125 0" rpy="0 0 0" />
                <axis xyz="0 1 0" />
            </xacro:if>
            <!-- FL-->
            <xacro:if value="${(mirror_dae == True) and (front_hind_dae == True)}">
                <origin xyz="0.08775 0.02125 0" rpy="0 0 0" />
                <axis xyz="0 -1 0" />
            </xacro:if>
            <!-- RR-->
            <xacro:if value="${(mirror_dae == False) and (front_hind_dae == False)}">
                <origin xyz="-0.08775 -0.02125 0" rpy="0 0 0" />
                <axis xyz="0 1 0" />
            </xacro:if>
            <!-- RL-->
            <xacro:if value="${(mirror_dae == True) and (front_hind_dae == False)}">
                <origin xyz="-0.08775 0.02125 0" rpy="0 0 0" />
                <axis xyz="0 -1 0" />
            </xacro:if>
            <parent link="${name}_hip" />
            <child link="${name}_thigh" />

            <dynamics damping="0.01" friction="0.2"/>
            <limit effort="18" lower="-3.14" upper="3.14" velocity="40"/>
            <!-- <limit effort="18" lower="-1.04" upper="1.04" velocity="40"/> -->
        </joint>

        <link name="${name}_thigh">
            <visual>
                <!-- FR-->
                <origin xyz="0 0 0" rpy="0 0 0" />
                <xacro:if value="${(mirror_dae == False)}">
                    <geometry>
                        <mesh filename="package://mz_description/meshes/thigh.dae" />
                    </geometry>
                    <!-- <collision>
                        <origin xyz="-0.014 -0.094 -0.14" rpy="0 0 0" />
                        <geometry>
                            <box size= "0.03 0.03 0.3"/>
                        </geometry>
                    </collision> -->
                </xacro:if>
                <!-- FL-->
                <xacro:if value="${(mirror_dae == True)}">
                    <geometry>
                        <mesh filename="package://mz_description/meshes/thigh_mirror.dae" />
                    </geometry>
                    <!-- <collision>
                        <origin xyz="-0.014 0.094 -0.14" rpy="0 0 0" />
                        <geometry>
                            <box size= "0.04 0.04 0.6"/>
                        </geometry>
                    </collision> -->
                </xacro:if>
                <!-- <collision>
                    <origin xyz="-0.014 -0.094 -0.15" rpy="0 0 0" />
                    <geometry>
                        <box size= "0.02 0.03 0.2"/>
                    </geometry>
                </collision> -->
                <material name="">
                    <color rgba="1 0 0 0.5" />
                </material>
            </visual>
            <collision>
                <origin xyz="0 0 -0.15" rpy="0 0 0" />
                <geometry>
                    <box size= "0.015 0.02 0.3"/>
                </geometry>
            </collision>
            <inertial>
                <origin xyz="-0.0037743 0.0038894 7.8535E-05" rpy="0 0 0" />
                <mass value="1.009" />
                <inertia ixx="0.004431760472" ixy="0.000057496807" ixz="-0.000218457134" iyy="0.004485671726" iyz="0.000572001265" izz="0.000740309489" />
            </inertial>
        </link>
        <joint name="${name}_calf_joint" type="revolute">
            <xacro:if value="${mirror_dae == True}">
                <origin xyz="0 0.09275 -0.3" rpy="0 0 0" />
                <axis xyz="0 -1 0" />
                <limit effort="27" lower="2.7" upper="0.785" velocity="26.6"/>
            </xacro:if>
            <xacro:if value="${mirror_dae == False}">
                <origin xyz="0 -0.09275 -0.3" rpy="0 0 0" />
                <axis xyz="0 1 0" />
                <limit effort="27" lower="-2.7" upper="-0.785" velocity="26.6"/>
            </xacro:if>
            <parent link="${name}_thigh" />
            <child link="${name}_calf" />

            <dynamics damping="0.01" friction="0.2"/>

        </joint>

        <link name="${name}_calf">
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0" />

                <geometry>
                    <mesh filename="package://mz_description/meshes/calf.dae" />
                </geometry>
                <material name="blue">
                    <color rgba="1 0 0 0.0" />
                </material>
            </visual>
            <collision>
                <origin xyz="0.0 0.0 -0.15" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <box size="0.015 0.002 0.3"/>
                </geometry>
            </collision>
            <!-- <collision>
                <origin xyz="0.004 0 -0.04" rpy="0 0 0" />
                <geometry>
                    <box size= "0.02 0.03 0.075"/>
                </geometry>
            </collision>
            <collision>
                <origin xyz="0.022 0 -0.13" rpy="0 -0.34 0" />
                <geometry>
                    <box size= "0.02 0.03 0.1"/>
                </geometry>
            </collision>
            <collision>
                <origin xyz="0.03 0 -0.240" rpy="0 0.35 0" />
                <geometry>
                    <box size= "0.02 0.03 0.08"/>
                </geometry>
            </collision> -->
            <inertial>
                <origin xyz="-0.0037743 0.0038894 7.8535E-05" rpy="0 0 0" />
                <mass value="0.224" />
                <inertia ixx="0.001088793059" ixy="-0.000000255679" ixz="0.000007117814" iyy="0.001100428748" iyz="0.000002077264" izz="0.000024787446" />
            </inertial>
        </link>

        <joint name="${name}_foot_fixed" type="fixed">
            <origin rpy="0 0 0" xyz="0 0 -0.3"/>
            <parent link="${name}_calf"/>
            <child link="${name}_foot"/>
        </joint>

        <link name="${name}_foot">
            <visual>
                <origin rpy="0 0 0" xyz="0 0 0"/>
                <geometry>
                    <sphere radius="0.01"/>
                </geometry>
                <material name="orange"/>
            </visual>
            <collision>
                <origin rpy="0 0 0" xyz="-0.003 0 0"/>
                <geometry>
                    <sphere radius="0.02"/>
                </geometry>
            </collision>
            <inertial>
                <mass value="0.01"/>
                <inertia ixx="9.6e-06" ixy="0.0" ixz="0.0" iyy="9.6e-06" iyz="0.0" izz="9.6e-06"/>
            </inertial>
        </link>

        <xacro:leg_transmission name="${name}"/>
    </xacro:macro>


    <!-- <xacro:macro name="leg" params="name"> -->
    <!-- <xacro:leg_transmission name="FR"/>
    <xacro:leg_transmission name="FL"/>
    <xacro:leg_transmission name="RR"/>
    <xacro:leg_transmission name="RL"/> -->
    <!-- </xacro:macro> -->
</robot>
    