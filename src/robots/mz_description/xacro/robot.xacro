        </geometry>
        </collision>
        <inertial>
        <origin rpy="0 0 0" xyz="-0.00401 -0.06043 -0.03327"/>
        <mass value="2.7"/>
        <inertia ixx="0.03077371" ixy="0" ixz="0" iyy="0.022321" iyz="0" izz="0.012608"/>
        </inertial>
    </link>

    <joint name="FR_calf_joint" type="revolute">
